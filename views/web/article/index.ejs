<table class="table table-hover">
<tbody><tr>
    <th>ID</th>
    <th>分类</th>
    <th>名称</th>
    <th>图片</th>

</tr>
<% data.forEach(function(item){ %>
<tr >
    <td><%= item._id %></td>
    <td><%= item.catid %></td>
    <td><a href="/article/view?id=<%= item._id %>"><%= item.name %></a></td>
    <td><img src="<%= item.image %>"  class="thumbnail" height="80"/></td>

</tr>
<%})%>



</tbody></table>


